{
  "name": "lumin-ui",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint-check": "prettier --check .",
    "lint-fix": "prettier --write .",
    "docs:dev": "vitepress dev packages/docs --port=3003",
    "docs:build": "vitepress build packages/docs",
    "docs:serve": "vitepress serve packages/docs",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/lumin-ui.umd.js",
  "module": "./dist/lumin-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/lumin-ui.es.js",
      "require": "./dist/lumin-ui.umd.js"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@mdx-js/react": "^2.1.1",
    "@storybook/addon-actions": "^6.5.3",
    "@storybook/addon-backgrounds": "^6.5.3",
    "@storybook/addon-docs": "^6.5.3",
    "@storybook/addon-essentials": "^6.5.3",
    "@storybook/addon-interactions": "^6.5.3",
    "@storybook/addon-links": "^6.5.3",
    "@storybook/addon-measure": "^6.5.3",
    "@storybook/addon-outline": "^6.5.3",
    "@storybook/addons": "^6.5.3",
    "@storybook/builder-vite": "^0.1.35",
    "@storybook/channel-postmessage": "^6.5.3",
    "@storybook/channel-websocket": "^6.5.3",
    "@storybook/client-api": "^6.5.3",
    "@storybook/core-common": "^6.5.3",
    "@storybook/preview-web": "^6.5.3",
    "@storybook/testing-library": "^0.0.11",
    "@storybook/vue3": "^6.5.3",
    "@types/node": "^17.0.35",
    "@vitejs/plugin-vue": "^2.3.3",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vitepress": "^0.22.4",
    "vitest": "^0.12.6",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.34.7"
  }
}
